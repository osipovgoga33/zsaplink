    setattributesfromstructure( node = ddena_node structure = ddena_wa ).
    rc = rootnode-&gt;append_child( ddena_node ).
  ENDLOOP.

*\--------------------------------------------------------------------/
  rc = xmldoc-&gt;append_child( rootnode ).
  ixmldocument = xmldoc.
ENDMETHOD.</source>

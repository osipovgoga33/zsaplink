  IF sy-subrc &lt;&gt; 0.
    CASE sy-subrc.
      WHEN 4.
        lv_textid = zcx_saplink=&gt;not_authorized.
      WHEN 5.
        lv_textid = zcx_saplink=&gt;not_found.
    ENDCASE.

    RAISE EXCEPTION TYPE zcx_saplink
        EXPORTING textid = lv_textid.
  ENDIF.
ENDMETHOD.</source>
